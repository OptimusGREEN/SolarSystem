# Used in a couple of places to get a set of can for current selected organisation
unsetenv ADMINORGANISATION
unsetenv CANEDITORGANISATION
unsetenv CANEDITACCESS
unsetenv CANEDITAREA
unsetenv CANEDITSITE
unsetenv CANEDITDEVICE
unsetenv CANEDITFOB
unsetenv CANEDITUSER
unsetenv CANARM
unsetenv CANDISARM
unsetenv CANUNLOCK
if($?SESSION_ORGANISATION) then
can --organisation='$SESSION_ORGANISATION' admin
if(! $status)setenv ADMINORGANISATION
can --organisation='$SESSION_ORGANISATION' editoriganisation
if(! $status)setenv CANEDITORGANISATION
can --organisation='$SESSION_ORGANISATION' editfob
if(! $status)setenv CANEDITFOB
can --organisation='$SESSION_ORGANISATION' edituser
if(! $status)setenv CANEDITUSER
can --organisation='$SESSION_ORGANISATION' viewlog
if(! $status)setenv CANVIEWLOG
can --organisation='$SESSION_ORGANISATION' arm
if(! $status)setenv CANARM
can --organisation='$SESSION_ORGANISATION' disarm
if(! $status)setenv CANDISARM
can --organisation='$SESSION_ORGANISATION' unlock
if(! $status)setenv CANUNLOCK
endif
if($?SESSION_SITE) then
can --organisation='$SESSION_ORGANISATION' --site='$SESSION_SITE' editaccess
if(! $status)setenv CANEDITACCESS
can --organisation='$SESSION_ORGANISATION' --site='$SESSION_SITE' editsite
if(! $status)setenv CANEDITSITE
can --organisation='$SESSION_ORGANISATION' --site='$SESSION_SITE' editdevice
if(! $status)setenv CANEDITDEVICE
can --organisation='$SESSION_ORGANISATION' --site='$SESSION_SITE' editarea
if(! $status)setenv CANEDITAREA
endif
